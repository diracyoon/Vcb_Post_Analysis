executable = Run_Job.sh
jobbatchname = Vcb_Histo_Smoothing
universe   = vanilla
arguments  = Histo_Syst.py -e $(Era) -ch $(Channel) -tagger $(Tagger) -smoothing 
log = condor.log
getenv     = True
transfer_input_files = Histo_Syst.py
should_transfer_files = YES
when_to_transfer_output = ON_EXIT
request_cpus = 1
output = job_Smoothing_$(Era)_$(Channel).log
error = job_Smoothing_$(Era)_$(Channel).log
concurrency_limits = n1000.isyoon

queue Era,Channel,Tagger from external.dat