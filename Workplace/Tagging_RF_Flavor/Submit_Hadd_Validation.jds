executable = Run_Job.sh
jobbatchname = Vcb_Tagging_RF_Hadd_Validation
universe   = vanilla
arguments  = Hadd_Tagging_RF.py -e $(Era) -ch $(Channel) -a Tagging_RF_Flavor -validation -tagger $(Tagger)
log = condor.log
getenv     = True
transfer_input_files = Hadd_Tagging_RF.py
should_transfer_files = YES
when_to_transfer_output = ON_EXIT
request_cpus = 1
output = job_Hadd_Validation_$(Era)_$(Channel).log
error = job_Hadd_Validation_$(Era)_$(Channel).err
concurrency_limits = n1100.isyoon

queue Era, Channel, Tagger from external_hadd.dat